<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dell.sampleregistration.NewBooking"
    android:background="@drawable/qaz">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Date"
        android:id="@+id/L1"
        android:textColor="@color/abc_primary_text_material_dark"
        android:textSize="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />


    <AutoCompleteTextView
        android:id="@+id/languages0"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_alignTop="@+id/L1"
        android:layout_alignStart="@+id/languages"
        android:hint="Enter Date" ></AutoCompleteTextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Time"
        android:id="@+id/L2"
        android:layout_below="@+id/L1"
        android:layout_marginTop="20dp"
        android:layout_alignParentStart="true"
        android:textColor="@color/abc_primary_text_material_dark"
        android:textSize="30dp" />


    <AutoCompleteTextView
        android:id="@+id/languages1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/L2"
        android:layout_alignEnd="@+id/languages"
        android:hint="Enter Time"></AutoCompleteTextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Duration"
        android:id="@+id/L3"
        android:textColor="@color/abc_primary_text_material_dark"
        android:textSize="30dp"

        android:layout_above="@+id/Button1"
        android:layout_alignParentStart="true" />


    <AutoCompleteTextView
        android:id="@+id/languages"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/L3"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/B1"
        android:hint="Enter Duration"></AutoCompleteTextView>


    <Button
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="Search"
        android:id="@+id/Button1"
        android:background="#4f7377"
        android:textColor="@color/abc_search_url_text_pressed"
        android:layout_above="@+id/B1"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="SLOT1"
        android:id="@+id/B1"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:onClick="onB1Click"
        android:longClickable="false"
        android:textColor="@color/abc_search_url_text_pressed" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="SLOT2"
        android:id="@+id/B2"
        android:layout_below="@+id/textView4"
        android:layout_alignParentStart="true"
        android:onClick="B2Clicked"
        android:textColor="@color/abc_search_url_text_pressed" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="SLOT3"
        android:id="@+id/B3"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/B2"
        android:onClick="B3Clicked"
        android:textColor="@color/abc_search_url_text_pressed" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="Slot4"
        android:id="@+id/B4"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignEnd="@+id/languages"
        android:onClick="B4Clicked"
        android:textColor="@color/abc_search_url_text_pressed" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="SLOT5"
        android:id="@+id/B5"
        android:layout_below="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:onClick="B5Clicked"
        android:textColor="@color/abc_search_url_text_pressed" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="Slot6"
        android:id="@+id/B6"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignEnd="@+id/languages"
        android:onClick="B6Clicked"
        android:textColor="@color/abc_search_url_text_pressed"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="|"
        android:id="@+id/textView3"
        android:layout_alignTop="@+id/B1"
        android:layout_centerHorizontal="true"
        android:textColor="@color/abc_search_url_text_pressed"
        android:textSize="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="|"
        android:id="@+id/textView4"
        android:layout_below="@+id/B1"
        android:layout_centerHorizontal="true"
        android:textSize="30dp"
        android:textColor="@color/abc_search_url_text_pressed" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="|"
        android:id="@+id/textView6"
        android:layout_below="@+id/B2"
        android:layout_centerHorizontal="true"
        android:textSize="30dp"
        android:textColor="@color/abc_search_url_text_pressed" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView7"
        android:textColor="@color/abc_search_url_text_pressed"
        android:textSize="30dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/B3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/B25"
        android:layout_alignTop="@+id/languages0"
        android:layout_toEndOf="@+id/languages0"
        android:background="@drawable/modified"
        android:nestedScrollingEnabled="false"
        android:onClick="onCalenderClick" />

</RelativeLayout>
