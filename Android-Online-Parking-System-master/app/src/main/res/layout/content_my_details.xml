<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.dell.sampleregistration.MyDetails2"
    tools:showIn="@layout/activity_my_details"
    android:background="@drawable/qaz">

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name:"
        android:id="@+id/M1"
        android:layout_marginTop="123dp"
        android:textIsSelectable="false"
        android:textSize="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Gautham"
        android:id="@+id/M2"
        android:layout_alignBottom="@+id/M1"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="43dp"
        android:textSize="30dp"
        android:textIsSelectable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="MY DETAILS"
        android:id="@+id/Heading"
        android:textSize="50dp"
        android:textIsSelectable="true"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/M2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Phone NO:"
        android:id="@+id/M3"
        android:layout_below="@+id/M1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="49dp"
        android:textSize="30dp"
        android:textIsSelectable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="9829193324"
        android:id="@+id/M4"
        android:textSize="30dp"
        android:layout_alignTop="@+id/M3"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Email Id:"
        android:id="@+id/M5"
        android:layout_below="@+id/M3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="58dp"
        android:textSize="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="abc@gmail.com"
        android:id="@+id/M6"
        android:layout_alignBottom="@+id/M5"
        android:layout_alignStart="@+id/M2"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="VALID BOOKINGS:"
        android:id="@+id/M7"
        android:layout_marginTop="56dp"
        android:layout_below="@+id/M5"
        android:layout_alignParentStart="true"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="1030 "
        android:id="@+id/M8"
        android:textSize="25dp"
        android:layout_alignTop="@+id/M7"
        android:layout_alignEnd="@+id/M2" />
</RelativeLayout>
